<%= form_for @post, html: {multipart: true} do |f| %>
  <!-- <%= f.hidden_field :name, :value => @user.profile_name %> -->
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.submit %>
<% end %>